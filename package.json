    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.0.5",
    "autoprefixer": "^10.4.20",
    "dependency-cruiser": "^16.9.0",
