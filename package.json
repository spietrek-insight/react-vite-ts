{
  "name": "react-vite-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test -c playwright-ct.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check:dupecode": "jscpd src",
    "pwa:test": "serve -s dist -l 60562",
    "server": "npx json-server data/db.json -p 3001",
    "dependency": "npx depcruise --config .dependency-cruiser.cjs --output-type json src > dependency-cruiser-output.json",
    "dupe:check": "jscpd --pattern \"src/**/*.ts*\" --ignore \"**/*.spec.ts\"",
    "yarn:upgrade": "yarn set version 4.5.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@tanstack/react-query": "^5.66.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "primeicons": "^7.0.0",
    "primereact": "10.9.2",
    "react": "^19.0.0",
    "react-doc-viewer": "^0.1.14",
    "react-dom": "^19.0.0",
    "react-ga4": "^2.1.0",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^5.4.0",
    "react-intl": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "uuid": "^11.0.5",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@playwright/experimental-ct-react": "^1.50.1",
    "@playwright/test": "^1.50.1",
    "@storybook/addon-a11y": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/addon-viewport": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
