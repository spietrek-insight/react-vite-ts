{
  "name": "react-vite-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test -c playwright-ct.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check:dupecode": "jscpd src",
    "pwa:test": "serve -s dist -l 60562",
    "server": "npx json-server data/db.json -p 3001",
    "dependency": "npx depcruise --config .dependency-cruiser.cjs --output-type json src > dependency-cruiser-output.json",
    "dupe:check": "jscpd --pattern \"src/**/*.ts*\" --ignore \"**/*.spec.ts\"",
    "yarn:upgrade": "yarn set version 4.5.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@tanstack/react-query": "^5.59.20",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "primeicons": "^7.0.0",
    "primereact": "10.8.4",
    "react": "^18.3.1",
    "react-doc-viewer": "^0.1.14",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^5.3.0",
    "react-intl": "^6.8.7",
    "react-router-dom": "^6.28.0",
    "uuid": "^11.0.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.14.0",
    "@playwright/experimental-ct-react": "^1.48.2",
    "@playwright/test": "^1.48.2",
    "@storybook/addon-a11y": "^8.4.2",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-links": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/addon-viewport": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
