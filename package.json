    "@storybook/react-vite": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-gtm-module": "^2.0.4",
    "@types/uuid": "^10.0.0",
