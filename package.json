    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/ui": "^2.1.4",
    "autoprefixer": "^10.4.20",
    "dependency-cruiser": "^16.6.0",
    "eslint": "9.14.0",
